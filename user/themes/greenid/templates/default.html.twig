{% extends 'partials/base.html.twig' %}

{% block content %}
<div id="page-content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                {% include 'partials/globalnav.html.twig' %}
                <h1>{{ page.title }}</h1>
                <div class="clearfix">

                <div id="contactbox" itemscope itemtype="http://schema.org/ProfessionalService">
                  <h5>Neem contact op</h5>
                  <h6 itemprop="founder">Ides De Vlamynck</h6>
                  <ul>
                    <li itemprop="telephone"><i class="fa fa-phone" aria-hidden="true"></i> 050/615 002</li>
                    <li itemprop="faxNumber"><i class="fa fa-fax" aria-hidden="true"></i> 050/615 006</li>
                    <li itemprop="telephone"><i class="fa fa-mobile" style="font-size:larger" aria-hidden="true"></i> 0495 68 57 13</li>
                    <li itemprop="email"><i class="fa fa-envelope-o" aria-hidden="true"></i><a style="color:#fff;text-decoration:none" href="mailto:info@greenid.be">info@greenid.be</a></li>
                  </ul>
                </div>

                <div class="welkom-tekst">
                  {{ page.content }}
                </div>



              </div>{# /end clearfix #}

                <div class="clearfix">
                  <div class="welkom-beeld">
                      <img src="{{page.media['welkom.jpg'].url}}" />
                  </div>



                </div>
            </div>
        </div>
    </div>

    {% include 'partials/footer.html.twig' %}

</div>
{% endblock %}
